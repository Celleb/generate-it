{
  "name": "openapi-nodegen",
  "main": "./lib/codegen.js",
  "version": "2.1.6",
  "description": "An OpenAPI 2/3 code generator for Node.js",
  "bin": {
    "openapi-nodegen": "./cli.js"
  },
  "scripts": {
    "build:tocs": "npx doctoc .",
    "install:githooks": "cp ./githooks/* .git/hooks",
    "start": "node cli.js",
    "test": "jest",
    "lint": "eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acrontum/openapi-nodegen"
  },
  "keywords": [
    "swagger",
    "openapi",
    "rest",
    "es6",
    "es7",
    "es2015",
    "node.js",
    "codegen",
    "generator"
  ],
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^2.12.2",
    "deepmerge": "^3.2.0",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.0.4",
    "inquirer": "^6.3.1",
    "js-beautify": "^1.8.8",
    "js-yaml": "^3.10.0",
    "json-schema-ref-parser": "^4.0.4",
    "lodash": "4.17.11",
    "md5-file": "^4.0.0",
    "nunjucks": "^3.2.0",
    "project-name-generator": "2.1.1",
    "recursive-copy": "^2.0.9",
    "uglify-es": "^3.3.9",
    "walk": "^2.3.14",
    "word-wrap": "1.1.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.16.0",
    "jest": "^24.7.1"
  }
}
