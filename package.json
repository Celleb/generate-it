{
  "name": "openapi-nodegen",
  "main": "./lib/codegen.js",
  "version": "2.7.7",
  "description": "An OpenAPI 2/3 code generator for Node.js",
  "bin": {
    "openapi-nodegen": "./cli.js"
  },
  "scripts": {
    "build:tocs": "npx doctoc .",
    "install:githooks": "cp ./githooks/* .git/hooks",
    "start": "node cli.js",
    "test": "jest",
    "lint": "eslint ./",
    "prepublish": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acrontum/openapi-nodegen"
  },
  "keywords": [
    "swagger",
    "openapi",
    "rest",
    "es6",
    "es7",
    "es2015",
    "node.js",
    "codegen",
    "generator"
  ],
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "deepmerge": "^4.0.0",
    "diff": "^4.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.5.0",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^7.1.0",
    "lodash": "^4.17.15",
    "md5-file": "^4.0.0",
    "nunjucks": "^3.2.0",
    "prettier": "^1.18.2",
    "progress": "^2.0.3",
    "project-name-generator": "^2.1.7",
    "recursive-copy": "^2.0.10",
    "uglify-es": "^3.3.9",
    "walk": "^2.3.14",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0"
  }
}
