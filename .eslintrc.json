{
  "extends": "standard",
  "parser": "babel-eslint",
  "rules": {
      "semi": "off",
      "no-new": 0,
      "no-process-env": "off",
      "quotes": "error",
      "space-before-function-paren": 0,
      "comma-dangle": [
          2,
          "always-multiline"
      ],
      "generator-star-spacing": 0
  },
  "env": {
      "node": true,
      "jest": true
  },
  "globals": {
      "$": false,
      "google": false,
      "_": false
  }
}
